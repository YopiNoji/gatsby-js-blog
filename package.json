{
  "name": "yopinoji.com",
  "version": "1.0.0",
  "description": "A tech blog that made by GatsbyJS.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YopiNoji/gatsby-js-blog.git"
  },
  "keywords": [
    "gatsby",
    "typescript",
    "tailwind",
    "blog"
  ],
  "author": "YopiNoji <yopinoji@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "npm run develop",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --fix",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop-in-container": "gatsby develop -H 0.0.0.0",
    "serve": "gatsby build && gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll"
  },
  "bugs": {
    "url": "https://github.com/YopiNoji/gatsby-js-blog/issues"
  },
  "homepage": "https://github.com/YopiNoji/gatsby-js-blog#readme",
  "dependencies": {
    "prismjs": "^1.21.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "gatsby": "^2.24.89",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-feed": "^2.5.11",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-manifest": "^2.4.27",
    "gatsby-plugin-offline": "^3.2.26",
    "gatsby-plugin-postcss": "^2.3.11",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.31",
    "gatsby-plugin-tsconfig-paths": "^1.0.2",
    "gatsby-plugin-typescript": "^2.4.18",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.13",
    "gatsby-remark-images": "^3.3.29",
    "gatsby-remark-prismjs": "^3.5.11",
    "gatsby-source-filesystem": "^2.3.27",
    "gatsby-transformer-remark": "^2.8.33",
    "gatsby-transformer-sharp": "^2.5.14",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "react-is": "^17.0.1",
    "tailwindcss": "^1.8.2",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix"
    ]
  }
}
