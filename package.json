{
  "name": "gatsby-js-blog",
  "description": "Gatsby blog",
  "version": "1.0.0",
  "author": "YopiNoji",
  "dependencies": {
    "gatsby": "^2.20.26",
    "gatsby-image": "^2.3.4",
    "gatsby-plugin-catch-links": "^2.2.3",
    "gatsby-plugin-feed": "^2.4.3",
    "gatsby-plugin-google-analytics": "^2.2.4",
    "gatsby-plugin-lodash": "^3.2.3",
    "gatsby-plugin-manifest": "^2.3.5",
    "gatsby-plugin-nprogress": "^2.2.3",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.2.4",
    "gatsby-plugin-sharp": "^2.5.6",
    "gatsby-plugin-sitemap": "^2.3.5",
    "gatsby-plugin-twitter": "^2.2.4",
    "gatsby-remark-autolink-headers": "^2.2.3",
    "gatsby-remark-copy-linked-files": "^2.2.3",
    "gatsby-remark-images": "^3.2.4",
    "gatsby-remark-prismjs": "^3.4.4",
    "gatsby-remark-responsive-iframe": "^2.3.3",
    "gatsby-source-filesystem": "^2.2.4",
    "gatsby-transformer-remark": "^2.7.3",
    "gatsby-transformer-sharp": "^2.4.6",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-share": "^2.4.0",
    "react-twitter-widgets": "^1.7.1",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "gh-pages": "^2.2.0",
    "husky": "4.2.0",
    "lint-staged": "10.0.2",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "remark-cli": "^8.0.0",
    "remark-preset-lint-recommended": "^3.0.4",
    "stylefmt": "^6.0.3",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "textlint": "11.6.1",
    "textlint-filter-rule-comments": "1.2.2",
    "textlint-filter-rule-whitelist": "2.0.0",
    "textlint-rule-ja-hiragana-fukushi": "1.2.0",
    "textlint-rule-ja-hiragana-hojodoushi": "1.0.4",
    "textlint-rule-ja-hiragana-keishikimeishi": "1.0.2",
    "textlint-rule-ja-no-space-around-parentheses": "2.0.1",
    "textlint-rule-ja-space-around-code": "2.0.1",
    "textlint-rule-preset-ja-spacing": "2.0.1",
    "textlint-rule-preset-ja-technical-writing": "3.1.3",
    "textlint-rule-preset-jtf-style": "2.3.4",
    "write-good": "^1.0.2"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean:old": "rm -rf public && rm -rf .cache",
    "clean": "gatsby clean",
    "eslint": "eslint --ext .js,.jsx . --fix",
    "lint:md": "remark content/**/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "prettier": "prettier '**/*.{js,jsx}' --write",
    "texlint:diff": "textlint --cache $(git diff master --name-only) --fix",
    "texlint:full": "textlint --cache README.md content/**/*.md --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.md": [
      "textlint --cache --fix"
    ],
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.jsx": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
